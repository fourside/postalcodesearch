{
  "name": "postalcodesearch",
  "version": "0.1.0",
  "bin": {
    "postalcodesearch": "bin/postalcodesearch.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint **/*.ts",
    "prettier": "prettier --write \"{**/*.ts,lib/lambdas/*.ts}\"",
    "precommit": "tsc --noEmit && lint-staged"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.61.1",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "aws-cdk": "1.61.1",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.61.1",
    "@aws-cdk/aws-lambda": "^1.61.1",
    "@aws-cdk/aws-lambda-nodejs": "^1.61.1",
    "@aws-cdk/aws-logs": "^1.61.1",
    "@aws-cdk/core": "1.61.1",
    "source-map-support": "^0.5.16"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run prettier"
    ]
  }
